<odoo>
    <data>
        <record model="ir.ui.view" id="view_students_tree">
            <field name="name">my.student.tree</field>
            <field name="model">my.student</field>
            <field name="arch" type="xml">
                <tree string="Students List">
                    <field name="name"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="view_students_form">
            <field name="name">my.student.form</field>
            <field name="model">my.student</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="birthdate"/>
                            <field name="address"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>